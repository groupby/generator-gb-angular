{
  "name": "<%= _.slugify(appname) %>",
  "version": "0.0.0",
  "main": "index.html",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "angular": "~1.3",
    "angular-animate": "~1.3",
    "angular-bootstrap": "~0.11.2",
    "angular-cookies": "~1.3",
    "angular-mocks": "~1.3",
    "angular-nouislider":"~0.3.1",
    "angular-resource": "~1.3",
    <% if(uirouter) { %>
    "angular-ui-router": "~0.2.11",
    <% } else { %>
    "angular-route": "~1.3",
    <% } %>
    "angular-ui-utils": "~0.1.1",
    "bootstrap": "~3",
    <% if(css === 'Sass') { %>
    "bootstrap-sass-official": "~3.2",
    <% } %>
    "font-awesome": "~4.2.0",
    "jquery": ">=2.0",
    "less.js": "~1.7",
    "moment": "~2.8",
    "underscore": "~1.7"
  },
  "resolutions": {
    "angular":"1.3.2"
  }
}
